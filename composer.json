{
    "name": "mallardduck/blade-mtg-icons",
    "description": "A package to easily make use of MTG Icons in your Laravel Blade views.",
    "keywords": ["Blade", "MTG Icons", "Laravel"],
    "homepage": "https://github.com/mallardduck/blade-mtg-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Blade Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "MallardDuck\\MTGIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MallardDuck\\MTGIcons\\BladeMTGIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
